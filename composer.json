{
    "name": "marcosh/fundic",
    "authors": [
        {
            "name": "Marco Perone",
            "email": "m.perone@mvlabs.it"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/container": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Fundic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FundicTest\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "ocramius/proxy-manager": "^2.1"
    },
    "suggest": {
        "ocramius/proxy-manager": "Allows you to use the Proxy factory decorator"
    }
}
